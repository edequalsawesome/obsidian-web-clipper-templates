{
	"schemaVersion": "0.1.0",
	"name": "Default",
	"behavior": "create",
	"noteContentFormat": "# {{title}}\n\n{{content}}",
	"properties": [
		{
			"name": "type",
			"value": "clippings",
			"type": "text"
		},
		{
			"name": "title",
			"value": "{{title}}",
			"type": "text"
		},
		{
			"name": "source",
			"value": "{{url}}",
			"type": "text"
		},
		{
			"name": "author",
			"value": "{{author|split:\\\", \\\"|wikilink|join}}",
			"type": "multitext"
		},
		{
			"name": "publishedDate",
			"value": "{{published}}",
			"type": "date"
		},
		{
			"name": "clipped",
			"value": "{{date}}",
			"type": "date"
		},
		{
			"name": "description",
			"value": "{{description}}",
			"type": "text"
		},
		{
			"name": "tldr",
			"value": "{{\\\"You are a savvy assistant -- give a brief 3-5 sentence overview of the content of the page like an executive recap\\\"}}",
			"type": "text"
		},
		{
			"name": "tags",
			"value": "clippings",
			"type": "multitext"
		}
	],
	"triggers": [],
	"noteNameFormat": "{{date|date:\"YYYY-MM-DD\"}} - {{title}}",
	"path": "Clippings"
}